{
  "name": "mnm-crm",
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://mnm-crm-frontend.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
